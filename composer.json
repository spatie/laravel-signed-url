{
    "name": "spatie/laravel-url-signer",
    "description": "Laravel implementation of spatie/signed-url",
    "keywords": [
        "spatie",
        "laravel-url-signer"
    ],
    "homepage": "https://github.com/spatie/laravel-url-signer",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "spatie/url-signer": "^1.0.1",
        "illuminate/support": "~5.4.0|~5.5.x-dev|~5.5.x-dev",
        "illuminate/http": "~5.4.0|~5.5.x-dev|~5.5.x-dev"
    },
    "require-dev": {
        "orchestra/testbench": "~3.4.4|~3.5.x-dev|~3.5.x-dev",
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\UrlSigner\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\UrlSigner\\Laravel\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Spatie\\UrlSigner\\Laravel\\UrlSignerServiceProvider"
            ],
            "aliases": {
                "UrlSigner": "Spatie\\UrlSigner\\Laravel\\UrlSignerFacade"
            }
        }
    }
}

